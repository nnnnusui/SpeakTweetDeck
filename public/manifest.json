{
  "name": "SpeakTweetDeck",
  "version": "1.1.1",
  "manifest_version": 3,
  "description": "Adds read-aloud functionality to TweetDeck.",
  "content_scripts": [{
    "matches": [ "https://tweetdeck.twitter.com/*" ],
    "css": ["style.css"], "js": ["main.js"]
  }],
  "icons": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
  }
}
